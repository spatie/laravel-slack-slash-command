{
    "name": "spatie/laravel-slack-slash-command-slash-command",
    "description": "Make a Laravel app respond to a slash command from Slack",
    "keywords": [
        "spatie",
        "laravel-slack"
    ],
    "homepage": "https://github.com/spatie/laravel-slack-slash-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/routing": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/config": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "illuminate/queue": "~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0",
        "guzzlehttp/guzzle": "^6.2.1"
        "php" : "^7.2",

    },
    "require-dev": {
        "orchestra/testbench":  "~3.2.0|~3.3.0|~3.4.0|~3.5.0|~3.6.0|~3.7.0",
        "mockery/mockery": "^1.0"
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SlashCommand\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SlashCommand\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\SlashCommand\\SlashCommandServiceProvider"
            ]
        }
    }
}
